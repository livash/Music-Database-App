<h2>Information about track "<%= @track.track_name %>"</h2>

	Band: <%= @track.band.band_name%>
	<br>
	Album: <%= @track.album.album_name %>
	<br>
	<br>
	<% if @track.notes %>
	Notes for this track: <%= @track.notes.map(&:body) %>
	<br>
	<br>
	<% end %>
	<br>
	<%= button_to "Delete Track", track_url(@track), :method => "delete" %>
	<br>
	<%= render :partial => "new_note" %>
	<br>
	<%= link_to "Make new note", new_note_url %>
	<br>
	<%= link_to "All Albums", albums_path %>
	<br>
	<%= link_to "All Bands", bands_path %>