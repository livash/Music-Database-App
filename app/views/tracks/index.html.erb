<h2>All tracks in your database</h2>

<table border="1" cellpadding="10">
	<tr>
	<th>ID</th>
	<th>Track</th>
	<th>Album</th>
	<th>Band</th>
	</tr>
<% @tracks.each do |track| %>
	<tr>
		<td>
			<%= track.id %>
		</td>
		<td>
			<%= track.track_name %>
		</td>
		<td>
			<%= track.album.album_name %>
		</td>
		<td>
			<%= track.band.band_name %>
		</td>

	</tr>
<% end %>
</table>